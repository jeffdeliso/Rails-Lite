#!/usr/bin/env ruby
require 'pry'
require 'rack'
require_relative '../lib/router/router'
require_relative '../config/routes'
Dir[File.join(__dir__, '..', 'app', 'controllers', '*.rb')].each { |file| require file }
Dir[File.join(__dir__, '..', 'app', 'models', '*.rb')].each { |file| require file }

Pry.start(__FILE__)